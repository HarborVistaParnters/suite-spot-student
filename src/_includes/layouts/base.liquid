<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% render 'partials/components/head_meta.liquid',
            data: data,
            site_settings: sanity.site_settings,
            document_ids: sanity.document_ids,
            page: page,
            eleventy: eleventy
        %}
        {% render 'partials/components/head_assets.liquid' %}
    </head>
    <body>
        {% render 'partials/components/parent_site_header.liquid', brands: brands %}
        {% assign main_navigation = sanity.navigation | find: '_id', sanity.document_ids.mainNav %}
        {% render 'partials/components/site_header.liquid', navigation: main_navigation %}
        {% block main %}
        {% endblock %}
        {% render 'partials/components/site_footer.liquid',
            navigation: main_navigation,
            site_settings: sanity.site_settings
        %}
    </body>
</html>
